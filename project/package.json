{
	"name": "projectname",
	"version": "0.0.0",
	"description": "projectdescription",
	"license": "MIT",
	"main": "dist/projectname.js",
	"module": "src/index.js",
	"engines": {
		"node": ">=6.0.0"
	},
	"_moduleAliases": {
		"projectname": "./src/index.js"
	},
	"nyc": {
		"temp-dir": "./coverage",
		"report-dir": "./coverage",
		"reporter": [
			"lcov",
			"text"
		]
	},
	"scripts": {
		"init": "rm -rf node_modules > /dev/null && npm i",
		"update": "npx npm-check-updates -u && npm install && npm update --dev && npm audit fix",
		"test": "eslint . && cross-env BROWSERSLIST_ENV=modern NODE_ENV=test BABEL_ENV=test node ./utils/run.js ./test/index.js | faucet && nyc report",
		"test-webpack": "eslint . && cross-env BROWSERSLIST_ENV=modern NODE_ENV=test BABEL_ENV=test webpack --config ./webpack/test.config.js && nyc --reporter=lcov node ./test/build/index.js | faucet && nyc report",
		"fix": "eslint --fix .",
		"coverage": "codecov",
		"build": "run-p build-js:*",
		"build-js:legacy": "cross-env BROWSERSLIST_ENV=legacy NODE_ENV=production BABEL_ENV=production webpack --config ./webpack/production.config.js",
		"build-js:modern": "cross-env BROWSERSLIST_ENV=modern NODE_ENV=production BABEL_ENV=production webpack --config ./webpack/production.config.js",
		"documentation": "jsdoc2md src/index.js > DOCUMENTATION.md",
		"release": "npm run release-patch",
		"release-nobump": "npm test && npm run build && npm run documentation && va-release --no-github --no-npm",
		"release-patch": "npm test && npm run build && npm run documentation && va-release --version patch",
		"release-minor": "npm test && npm run build && npm run documentation && va-release --version minor",
		"release-major": "npm test && npm run build && npm run documentation && va-release --version major",
		"build-templates": "va-release -t",
		"copyfiles": "copyfiles -f ./src/sw.js ./dist/",
		"dev:js": "./utils/run.js ./build-utils/dev-server.js",
		"dev:less": "watch -p './src/less/*.less' -c 'npm run build-and-minify:less'",
		"minify:less": "foreach -g './dist/css/index.css' -i './dist/css/index.min.css' -x 'uglifycss #{path} --output #{dir}/#{name}.min#{ext}'",
		"build-and-minify:less": "run-p build:less minify:less"
	},
	"va-release": {
		"assets": "dist/**/*",
		"files": [
			{
				"cwd": "templates/",
				"src": [
					"**/*"
				],
				"dest": "./"
			}
		]
	},
	"keywords": [
	],
	"dependencies": {},
	"devDependencies": {
		"autoprefixer": "^9.1.5",
		"jsdoc-to-markdown": "^4.0.1",
		"@babel/core": "^7.4.3",
		"@babel/plugin-proposal-object-rest-spread": "^7.4.3",
		"@babel/plugin-proposal-optional-chaining": "^7.2.0",
		"@babel/plugin-proposal-unicode-property-regex": "^7.4.0",
		"@babel/plugin-syntax-dynamic-import": "^7.2.0",
		"@babel/plugin-syntax-import-meta": "^7.2.0",
		"@babel/plugin-transform-regenerator": "^7.4.3",
		"@babel/polyfill": "^7.4.3",
		"@babel/preset-env": "^7.4.3",
		"@babel/register": "^7.4.0",
		"babel-eslint": ">=10.0.1",
		"babel-loader": "^8.0.5",
		"babel-minify": "^0.5.0",
		"babel-minify-webpack-plugin": "^0.3.1",
		"babel-plugin-istanbul": "^5.1.2",
		"babel-preset-minify": "^0.5.0",
		"browser-sync": "^2.24.7",
		"codecov": "^3.3.0",
		"core-js": "^3.0.1",
		"cross-env": "^5.2.0",
		"deepmerge": "^3.2.0",
		"eslint": ">=5.16.0",
		"esm": "^3.2.22",
		"faucet": "0.0.1",
		"foreach-cli": "^1.8.1",
		"fs-extra": "^7.0.1",
		"globby": "^9.2.0",
		"imports-loader": "^0.8.0",
		"inject-loader": "^4.0.1",
		"http-proxy-middleware": "^0.19.0",
		"find-free-port": "^2.0.0",
		"module-alias": "^2.2.0",
		"npm-run-all": "^4.1.5",
		"nyc": "^14.0.0",
		"npm-check-updates": "^3.1.8",
		"sinon": "^7.3.1",
		"tape": "^4.10.1",
		"terser-webpack-plugin": "^1.2.3",
		"universal-url": "^2.0.0",
		"va-release": "^1.1.9",
		"postcss-cli": "^6.0.0",
		"ngrok": "^3.1.0",
		"opn": "^5.4.0",
		"watch-cli": "^0.2.3",
		"webpack": "4.30.0",
		"webpack-dev-middleware": "3.4.0",
		"webpack-dev-server": "3.1.8",
		"webpack-hot-middleware": "2.24.2",
		"webpack-bundle-analyzer": "^3.3.2",
		"webpack-cli": "3.3.0",
		"webpack-merge": "4.2.1"
	},
	"tonicExampleFilename": "tonic-example.js"
}
