{
	"name": "projectname",
	"version": "0.0.0",
	"description": "projectdescription",
	"license": "MIT",
	"main": "dist/projectname.js",
	"module": "src/index.js",
	"engines": {
		"node": ">=10.11.0"
	},
	"_moduleAliases": {
		"projectname": "./src/index.js"
	},
	"nyc": {
		"temp-dir": "./coverage",
		"report-dir": "./coverage",
		"reporter": [
			"lcov",
			"text"
		]
	},
	"scripts": {
		"init": "rm -rf node_modules > /dev/null && npm i",
		"start": "run-s start:*",
		"build": "run-s build:*",
		"bump": "npm version patch --no-git-tag-version",
		"update": "npx npm-check-updates -u && npm install && npm update --dev && npm audit fix",
		"test": "eslint . && run-p test:*",
		"fix": "eslint --fix .",
		"coverage": "codecov",
		"release": "npm run release-patch",
		"release-nobump": "npm test && npm run build && npm run documentation && va-release --no-github --no-npm",
		"release-patch": "npm test && npm run build && npm run documentation && va-release --version patch",
		"release-minor": "npm test && npm run build && npm run documentation && va-release --version minor",
		"release-major": "npm test && npm run build && npm run documentation && va-release --version major",
		"build-templates": "va-release -t"
	},
	"va-release": {
		"assets": "dist/**/*",
		"files": [
			{
				"cwd": "templates/",
				"src": [
					"**/*"
				],
				"dest": "./"
			}
		]
	},
	"keywords": [],
	"dependencies": {},
	"devDependencies": {
		"cross-env": "^5.2.0",
		"deepmerge": "^3.2.0",
		"eslint": ">=5.16.0",
		"babel-eslint": ">=10.0.1",
		"esm": "^3.2.22",
		"faucet": "0.0.1",
		"fs-extra": "^7.0.1",
		"globby": "^9.2.0",
		"module-alias": "^2.2.0",
		"npm-run-all": "^4.1.3",
		"sinon": "^7.3.2",
		"tape": "^4.10.1",
		"universal-url": "^2.0.0",
		"va-release": "^1.1.14"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/projectowner/projectname.git"
	},
	"bugs": {
		"url": "https://github.com/projectowner/projectname/issues"
	},
	"homepage": "https://github.com/projectowner/projectname#readme",
	"tonicExampleFilename": "tonic-example.js"
}
