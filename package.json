{
	"name": "va-create",
	"description": "",
	"version": "0.0.15",
	"main": "index.mjs",
	"engines": {
		"node": ">=10.11.0"
	},
	"bin": {
		"va-create": "./cli.mjs"
	},
	"esm": {
		"sourceMap": true,
		"cache": false,
		"cjs": true,
		"mode": "auto"
	},
	"_moduleAliases": {
		"va-create": "./index.js"
	},
	"va-release": {
		"owner": "forceuser",
		"library": "vaCreate"
	},
	"scripts": {
		"link": "npm link",
		"exec": "node ./utils/run.js ./index.js",
		"init": "rm -rf node_modules > /dev/null && npm i && npm link",
		"update": "npx npm-check-updates -u && npm install && npm update --dev && npm audit fix",
		"test": "eslint .",
		"fix": "eslint --fix .",
		"release": "npm run release-patch",
		"release-nobump": "npm test && va-release --no-github --no-npm",
		"release-patch": "npm test && va-release --version patch",
		"release-minor": "npm test && va-release --version minor",
		"release-major": "npm test && va-release --version major",
		"build-templates": "va-release -t"
	},
	"author": "",
	"license": "MIT",
	"dependencies": {
		"camelcase": "^5.3.1",
		"colors": "^1.3.3",
		"deepmerge": "^4.0.0",
		"esm": "^3.2.25",
		"fs-extra": "^8.1.0",
		"globby": "^10.0.1",
		"inquirer": "^7.0.0",
		"is-wsl": "^2.1.0",
		"module-alias": "^2.2.1",
		"mustache": "^3.1.0",
		"open": "^6.4.0",
		"readline-promise": "^1.0.4",
		"shelljs": "^0.8.3",
		"universal-url": "^2.0.0",
		"wsl-path": "^2.0.0",
		"yargs": "^14.0.0"
	},
	"devDependencies": {
		"babel-eslint": ">=10.0.3",
		"c8": "^5.0.4",
		"cross-env": "^5.2.1",
		"eslint": ">=6.4.0",
		"nyc": "^14.1.1",
		"va-release": "^1.1.15"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/forceuser/va-create.git"
	},
	"bugs": {
		"url": "https://github.com/forceuser/va-create/issues"
	},
	"homepage": "https://github.com/forceuser/va-create#readme"
}
