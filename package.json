{
	"name": "va-create",
	"version": "0.0.1",
	"description": "",
	"main": "index.js",
	"bin": "./cli.js",
	"_moduleAliases": {
		"va-create": "./index.js"
	},
	"va-release": {
		"owner": "forceuser",
		"library": "vaCreate"
	},
	"scripts": {
		"exec": "node ./utils/run.js ./index.js",
		"init": "rm -rf node_modules > /dev/null && npm i",
		"update": "npm-check-updates -u && npm install && npm update --dev && npm audit fix",
		"test": "eslint .",
		"fix": "eslint --fix .",
		"release": "npm run release-patch",
		"release-nobump": "npm test && va-release --no-github --no-npm",
		"release-patch": "npm test && va-release --version patch",
		"release-minor": "npm test && va-release --version minor",
		"release-major": "npm test && va-release --version major",
		"build-templates": "va-release -t"
	},
	"author": "",
	"license": "MIT",
	"dependencies": {
		"camelcase": "^5.3.1",
		"colors": "^1.3.3",
		"core-js": "^3.0.1",
		"form-data": "^2.3.3",
		"fs-extra": "^7.0.1",
		"isomorphic-fetch": "github:forceuser/isomorphic-fetch",
		"parse-github-url": "^1.0.2",
		"prompt": "^1.0.0",
		"readline-promise": "^1.0.4",
		"shelljs": "^0.8.3",
		"terminal-kit": "^1.28.0",
		"yargs": "^13.2.2"
	},
	"devDependencies": {
		"@babel/core": "^7.4.3",
		"@babel/plugin-proposal-object-rest-spread": "^7.4.3",
		"@babel/plugin-proposal-optional-chaining": "^7.2.0",
		"@babel/plugin-proposal-unicode-property-regex": "^7.4.0",
		"@babel/plugin-syntax-dynamic-import": "^7.2.0",
		"@babel/plugin-syntax-import-meta": "^7.2.0",
		"@babel/plugin-transform-regenerator": "^7.4.3",
		"@babel/polyfill": "^7.4.3",
		"@babel/preset-env": "^7.4.3",
		"@babel/register": "^7.4.0",
		"babel-eslint": ">=10.0.1",
		"babel-plugin-istanbul": "^5.1.2",
		"cross-env": "^5.2.0",
		"deepmerge": "^3.2.0",
		"eslint": ">=5.16.0",
		"esm": "^3.2.22",
		"foreach-cli": "^1.8.1",
		"fs-extra": "^7.0.1",
		"globby": "^9.2.0",
		"module-alias": "^2.2.0",
		"npm-check-updates": "^3.1.8",
		"npm-run-all": "^4.1.5",
		"universal-url": "^2.0.0",
		"va-release": "^1.1.12"
	}
}
